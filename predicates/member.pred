(lambda (x list)
	(setf result NIL)
	(let ((args (get list 'lexer::args)))
		(do* ((current (car args)))
			((not args) NIL)
			(setf args (cdr args))
			(cond ((equal (get current 'lexer::type) 'parser::const) (if (string-equal (get x 'lexer::name) (get current 'lexer::name)) (setf result T)))
				((equal (get current 'lexer::type) 'function) (append args (get current 'lexer::args)))
				(T NIL)
			)
		)
	)
	result
)
