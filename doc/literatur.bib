 issue_date = {Jan. 1965},
 volume = {12},
 number = {1},
 month = jan,
 year = {1965},
 issn = {0004-5411},
 pages = {23--41},
 numpages = {19},
 url = {http://doi.acm.org/10.1145/321250.321253},
 doi = {10.1145/321250.321253},
 acmid = {321253},
 publisher = {ACM},
 address = {New York, NY, USA},
} 

